<p id="notice"><%= notice %></p>

<h1>Inasistencia</h1>

<table>
  <thead>
    <tr>
      <th>Personal</th>
      <th>Fechadesde</th>
      <th>Fechahasta</th>
      <th>Causa</th>
      <th>Aviso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inasistencia.each do |inasistencium| %>
      <tr>
        <td><%= inasistencium.personal_id %></td>
        <td><%= inasistencium.fechadesde %></td>
        <td><%= inasistencium.fechahasta %></td>
        <td><%= inasistencium.causa %></td>
        <td><%= inasistencium.aviso %></td>
        <td><%= link_to 'Show', inasistencium %></td>
        <td><%= link_to 'Edit', edit_inasistencium_path(inasistencium) %></td>
        <td><%= link_to 'Destroy', inasistencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inasistencium', new_inasistencium_path %>
